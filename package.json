{
  "name": "opengui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npx kill-port 3001 && rsbuild build && rslib build && node server-dist/index.js",
    "dev:server": "node server-dist/index.js",
    "dev:build-serve": "rsbuild build && rslib build && node server-dist/index.js",
    "dev:full": "rsbuild build && rslib build && node server-dist/index.js",
    "build": "rsbuild build && rslib build && (cp -r data server-dist/ 2>/dev/null || true)",
    "start": "npx kill-port 3001 && cd server-dist && node index.js",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,md}\"",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "test": "rstest",
    "test:components": "rstest test/components",
    "test:runtime": "rstest test/runtime",
    "test:stores": "rstest test/stores",
    "test:integration": "rstest test/integration",
    "test:api": "rstest test/api",
    "test:server": "rstest test/server",
    "test:utils": "rstest test/utils",
    "test:util": "rstest test/util",
    "test:validation": "rstest test/validation",
    "test:parallel": "rstest --maxConcurrency=4",
    "test:verbose": "rstest --reporter=verbose",
    "test:only": "rstest --testNamePattern=only",
    "pretest:e2e": "npx kill-port 3001",
    "test:e2e": "pnpm build && npx playwright test",
    "test:visual": "pnpm build && npx playwright test test/e2e/visual/ui-screenshots.e2e.ts --project=chromium --reporter=line",
    "test:e2e:ci": "pnpm build && npx playwright test",
    "health-check": "tsx scripts/health-check.ts"
  },
